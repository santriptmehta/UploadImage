        android:theme="@style/Theme.UploadImage">
        <activity
            android:name="com.dhl.uploadimage_Assignment.registerActivity"
            android:exported="false" />
        <activity
            android:name="com.dhl.uploadimage_Assignment.loginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dhl.uploadimage_Assignment.MainActivity"
            android:exported="false">

        </activity>
    </application>

</manifest>